{
  "manifest_version": 3,
  "name": "Instagram Login",
  "version": "1.0",
  "description": "Developed By Tejas Hande",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["cookies", "tabs", "storage"]
}